{
  "name": "Mute Tab",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["activeTab"],
  "background": {
    "service_worker": "scripts/background.js"
  },
  "commands": {
    "mute-tab": {
      "suggested_key": {
        "default": "Ctrl+8"
      },
      "description": "Mute the current tab",
      "global": true
    }
  }
}